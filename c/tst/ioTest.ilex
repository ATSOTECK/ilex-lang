use { input, getNumber } from <io>

use <math as M>

fn test() {
    str := input('Enter something: ')
    println('You entered', str)
    
    num := str.toNumber() ?? 0
    
    println(num + 1)
    
    println(M::pi)
    
    num = getNumber("Enter a number: ")
    assert(typeof(num) == 'number')
    println('You entered', num)
}

