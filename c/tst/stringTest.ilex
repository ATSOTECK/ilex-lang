hi := "hi".toUpper()
assert(hi == "HI")

hi = "HI".toLower()
assert(hi == "hi")

len := "yeet".len()
assert(len == 4)

assert("This is a string".contains("is"))

str := "yeet"
assert(str.toUpper() == "YEET");

str = "YEET"
assert(str.toLower() == "yeet");

assert(str.len() == 4)

str = "This is a string"
assert(str.contains("a"))

println("String test passed in", milliseconds(), "ms!")

